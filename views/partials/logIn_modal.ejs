<button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#logIn">Log in</button>


<div id="logIn" class="modal fade" role="dialog">
  <div class="modal-dialog">

    <div class="modal-content">
      <div class="modal-header">
        <h3 class="modal-title">Welcome back!</h3>
        <button type="button" class="close" data-dismiss="modal">&times;</button>
      </div>
      <div class="modal-body">
        <form>
          <input type="string" placeholder="Email"><br/>
          <input type="string" placeholder="Enter your password">
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-link" data-dismiss="modal" data-toggle="modal" data-target="#signUp">Not a member? Sign up</button>
        <a href="/home"><button type="button" class="btn btn-outline-primary">Continue</button></a>
      </div>
    </div>

  </div>
</div>
